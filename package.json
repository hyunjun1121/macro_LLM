{
  "name": "web-agent",
  "version": "1.0.0",
  "description": "LLM-based web automation macro generator",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "test": "node src/test.js",
    "benchmark": "node src/benchmarkRunner.js",
    "benchmark:test": "node src/testBenchmark.js",
    "benchmark:youtube": "node src/benchmarkRunner.js youtube",
    "benchmark:all": "node src/benchmarkRunner.js",
    "extract-tasks": "node -e \"import('./src/taskExtractor.js').then(m => new m.TaskExtractor().discoverAllTasks().then(console.log))\"",
    "multi-model:fast": "node src/multiModelBenchmark.js fast",
    "multi-model:quality": "node src/multiModelBenchmark.js quality",
    "multi-model:reasoning": "node src/multiModelBenchmark.js reasoning",
    "multi-model:coding": "node src/multiModelBenchmark.js coding",
    "multi-model:anthropic": "node src/multiModelBenchmark.js anthropic",
    "multi-model:openai": "node src/multiModelBenchmark.js openai",
    "multi-model:google": "node src/multiModelBenchmark.js google",
    "multi-model:custom": "node src/multiModelBenchmark.js custom"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "glob": "^11.0.3",
    "openai": "^4.28.0",
    "playwright": "^1.40.0",
    "xlsx": "^0.18.5"
  }
}

unt = await page.$$eval('input[type=\"radio\"]', els => els.length);\n\n    // Extract all available language links (bottom of page, e.g., English, Español, etc.)\n    const languageLinks = await page.$$eval('a', els =>\n      els\n        .map(el => el.textContent.trim())\n        .filter(txt =>\n          ['English (US)', 'Español', 'Français (France)', '中文(简体)', 'العربية', 'Português (Brasil)', '한국어', 'Italiano', 'Deutsch', 'हिन्दी', '日本語'].includes(txt)\n        )\n    );\n\n    return {\n      success: true,\n      action: \"Filled login form, clicked Log In, clicked Create new account, and extracted login error, radio button count, and language links.\",\n      extractedData: {\n        loginError,\n        radioCount,\n        languageLinks\n      },\n      screenshots,\n      error: null\n    };\n  } catch (error) {\n    return {\n      success: false,\n      action: \"Failed to complete task\",\n      extractedData: {},\n      screenshots: [],\n      error: error.message\n    };\n  }\n}",

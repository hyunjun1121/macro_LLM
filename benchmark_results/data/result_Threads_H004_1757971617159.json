) content = contentEl.textContent.trim();\n\n      // Likes\n      let likes = null;\n      const likeEl = node.querySelector('.thread-action[data-action=\"like\"] span') ||\n                     node.querySelector('.thread-action.liked span') ||\n                     node.querySelector('span');\n      if (likeEl && !isNaN(Number(likeEl.textContent.trim()))) likes = Number(likeEl.textContent.trim());\n\n      // Replies\n      let replies = null;\n      const replyEl = node.querySelector('.thread-action[data-action=\"reply\"] span');\n      if (replyEl && !isNaN(Number(replyEl.textContent.trim()))) replies = Number(replyEl.textContent.trim());\n\n      // Reposts\n      let reposts = null;\n      const repostEl = node.querySelector('.thread-action[data-action=\"repost\"] span');\n      if (repostEl && !isNaN(Number(repostEl.textContent.trim()))) reposts = Number(repostEl.textContent.trim());\n\n      return {\n        username,\n        handle,\n        time,\n        content,\n        likes,\n        replies,\n        reposts\n      };\n    }));\n\n    // 3. Take a screenshot of all threads area\n    const threadsFeed = await page.$('#threads-feed') || await page.$('.threads-list') || await page.$('.main-content');\n    if (threadsFeed) {\n      await threadsFeed.screenshot({ path: path.join(screenshotsDir, 'step_3_threads_feed.png') });\n      screenshots.push(path.join(screenshotsDir, 'step_3_threads_feed.png'));\n    }\n\n    // 4. Try to extract a thread using XPath as an alternative strategy\n    const [xpathThread] = await page.$x('//div[contains(@class,\"thread-item\")]');\n    if (xpathThread) {\n      await xpathThread.screenshot({ path: path.join(screenshotsDir, 'step_4_xpath_thread.png') });\n      screenshots.push(path.join(screenshotsDir, 'step_4_xpath_thread.png'));\n    }\n\n    // 5. Try to extract a thread by searching for a known text (e.g. \"Coffee and coding\")\n    let coffeeThreadContent = null;\n    const coffeeThread = await page.$(`text=\"Coffee and coding\"`);\n    if (coffeeThread) {\n      coffeeThreadContent = await coffeeThread.textContent();\n      await coffeeThread.screenshot({ path: path.join(screenshotsDir, 'step_5_coffee_thread.png') });\n      screenshots.push(path.join(screenshotsDir, 'step_5_coffee_thread.png'));\n    }\n\n    return {\n      success: true,\n      action: \"Extracted all visible thread content data from the page using multiple selector strategies.\",\n      extractedData: {\n        threads: threadsData,\n        coffeeThreadContent\n      },\n      screenshots,\n      error: null\n    };\n  } catch (error) {\n    return {\n      success: false,\n      action: \"Failed to complete task\",\n      extractedData: {},\n      screenshots,\n      error: error.message\n    };\n  }\n}",
